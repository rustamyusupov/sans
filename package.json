{
  "name": "sans-cli",
  "version": "0.2.1",
  "author": "Rustam Yusupov",
  "description": "set StAtus iN Slack from the command line interface",
  "license": "MIT",
  "scripts": {
    "start": "npx babel-node -- src/bin/sans.js",
    "lint": "eslint .",
    "test": "jest",
    "watch": "jest --watch",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublishOnly": "yarn build"
  },
  "bin": {
    "sans": "dist/bin/sans.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "slack",
    "status",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rustamyusupov/sans.git"
  },
  "bugs": {
    "url": "https://github.com/rustamyusupov/sans/issues"
  },
  "homepage": "https://github.com/rustamyusupov/sans#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "commander": "^3.0.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.18.2"
  }
}
